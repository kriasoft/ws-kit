{
  "name": "@ws-kit/client",
  "version": "0.0.0",
  "description": "Universal WebSocket client for @ws-kit",
  "type": "module",
  "sideEffects": false,
  "license": "MIT",
  "author": "Konstantin Tarkus",
  "homepage": "https://kriasoft.com/ws-kit/",
  "bugs": "https://github.com/kriasoft/ws-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/kriasoft/ws-kit.git",
    "directory": "packages/client"
  },
  "keywords": [
    "websocket",
    "client",
    "browser",
    "real-time",
    "type-safe",
    "typescript"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./zod": {
      "types": "./dist/zod/index.d.ts",
      "default": "./dist/zod/index.js"
    },
    "./valibot": {
      "types": "./dist/valibot/index.d.ts",
      "default": "./dist/valibot/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {},
  "engines": {
    "node": ">=22"
  }
}
