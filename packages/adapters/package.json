  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/koistya"
  },
  "keywords": [
    "websocket",
    "adapter",
    "rate-limiting",
    "rate-limiter",
    "token-bucket",
    "redis",
    "durable-objects",
